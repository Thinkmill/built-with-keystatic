---
const seoImage = `${Astro.site}seo-image.png`;
---

<>
  {
    Astro.site?.origin === "https://builtwith.keystatic.com" ? (
      <meta content="index, follow" name="robots" />
    ) : (
      <meta content="noindex, nofollow" name="robots" />
    )
  }
  <meta
    content="A collection of projects using Keystatic to manage parts of their codebase."
    name="description"
  />
  <meta content="Built With Keystatic " property="og:title" />
  <meta content="website" property="og:type" /><meta
    content={seoImage}
    property="og:image"
  />
  <meta
    content={`${Astro.site}" property="og:url" /`}
    <meta
    content="A collection of projects using Keystatic to manage parts of their codebase."
    property="og:description"
  />
  <meta content={seoImage} property="og:image:url" />
  <meta content="1200" property="og:image:width" />
  <meta content="1200" property="og:image:height" />
  <meta content="Built with Keystatic" property="og:image:alt" />

  <meta content="summary_large_image" name="twitter:card" />
  <meta content="@thekeystatic" name="twitter:site" />
  <meta content="@thekeystatic" name="twitter:creator" />
  <meta content="Keystatic" name="twitter:title" />
  <meta
    content="A collection of projects using Keystatic to manage parts of their codebase."
    name="twitter:description"
  />
  <meta content={seoImage} name="twitter:image" />
</>
